/*文字省略*/
.tx_l1{
	text-overflow: ellipsis;
    white-space: nowrap;
    word-break: keep-all;
	}
.tx_l2{
	text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
	}
.tx_l3{
	text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
	}
